log4j.logger.calPolyScheduler=debug
log4j.rootLogger=debug, tail

log4j.appender.tail=com.amazonaws.services.lambda.runtime.log4j.LambdaAppender
#log4j.appender.tail.File=${catalina.base}/logs/calPolyScheduler.log
log4j.appender.tail.layout=org.apache.log4j.PatternLayout
log4j.appender.tail.layout.ConversionPattern=%d: %-5p %c{1} - %m%n